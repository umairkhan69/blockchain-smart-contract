pragma solidity >= 0.6.0 <0.9.0;

contract cal{
  uint num1 = 20;
  uint num2 =5;
         // +
    function adding() public view returns (uint ans){
        ans= num1 + num2  ;
        return ans;
    }
    function subtract() public view returns (uint ans){
        ans= num1 - num2 ;
        return ans;

    }
    function multi() public view returns (uint ans){
        ans= num1 * num2 ;
        return ans;
    }
    function divide() public view returns (uint ans){
        ans= num1 / num2 ;
        return ans;
    }


    
}
